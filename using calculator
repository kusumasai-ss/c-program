#include<stdio.h>
int main ()
{
	int a,b,c;
	char ch;
	printf("enter a,b values");
	scanf("%d%d",&a,&b);
	printf("enter any choice +add,-sub,*mulit,%div");
	scanf("%c",&ch);
	switch(ch)
	{
	case'+':c=a+b;
	printf("sum=%d",c);
	break;
	case'-':c=a-b;
	printf("sub=%d",c);
	break;
	case'*':c=a*b;
	printf("mult=%d",c);
	break;
	case'/':c=a/b;
	printf("div=%d",c);
	break;
	default:printf("invaluid");
	}
	return 0;
}
