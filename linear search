#include <stdio.h>

int main()
{
    int a[100], i, n, x, flag = 0;

    printf("Enter array size: ");
    scanf("%d", &n);

    printf("Enter array values:\n");
    for (i = 0; i < n; i++)
    {
        scanf("%d", &a[i]);
    }

    printf("Enter search element: ");
    scanf("%d", &x);

    for (i = 0; i < n; i++)
    {
        if (x == a[i])
        {
            printf("Element is found at position %d\n", i + 1);
            flag = 1;
            break;
        }
    }

    if (flag == 0)
        printf("Element is not found\n");

    return 0;
}
